@model Tarea34.Controllers.CedView

<h1 class="titlee">Usted ha de tener esta lista pendiente: </h1>
    <link href="~/lib/bootstrap/dist/css/InfoPrograma.css" rel="stylesheet" />
    <div class="area">
        <ul class="circles">
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
            <li></li>
